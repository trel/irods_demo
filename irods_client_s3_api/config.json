{
    "s3_server": {
        "host": "0.0.0.0",
        "port": 9000,
        "log_level": "info",
        "bucket_mapping": {
            "plugin_path": "/usr/lib/irods_s3_api/plugins/bucket_mapping/libirods_s3_api_plugin-bucket_mapping-local_file.so",
            "configuration": {
                "file_path": "/bucket_mappings.json"
            }
        },
        "user_mapping": {
            "plugin_path": "/usr/lib/irods_s3_api/plugins/user_mapping/libirods_s3_api_plugin-user_mapping-local_file.so",
            "configuration": {
                "file_path": "/user_mappings.json"
            }
        },
        "region": "us-east-1",
        "multipart_upload_part_files_directory": "/tmp",
        "requests": {
            "threads": 3,
            "max_size_of_request_body_in_bytes": 8388608,
            "timeout_in_seconds": 30
        },
        "background_io": {
            "threads": 6
        }
    },
    "irods_client": {
        "host": "irods-catalog-provider",
        "port": 1247,
        "zone": "tempZone",
        "enable_4_2_compatibility": false,
        "proxy_admin_account": {
            "username": "rods",
            "password": "rods"
        },
        "connection_pool": {
            "size": 6,
            "refresh_timeout_in_seconds": 600,
            "max_retrievals_before_refresh": 16,
            "refresh_when_resource_changes_detected": true
        },
        "resource": "demoResc",
        "put_object_buffer_size_in_bytes": 8192,
        "get_object_buffer_size_in_bytes": 8192
    }
}
